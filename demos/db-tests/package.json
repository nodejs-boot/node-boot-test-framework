{
    "name": "@nodeboot/db-tests",
    "version": "1.5.0",
    "description": "Demo project for Node-Boot tests with Jest",
    "author": "Manuel Santos <ney.br.santos@gmail.com>",
    "license": "MIT",
    "private": "true",
    "keywords": [
        "tests",
        "node-boot"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nodejs-boot/node-boot-test-framework.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "types": "src/index.ts",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "clean:build": "rimraf ./dist",
        "dev": "nodemon",
        "lint": "eslint . --ext .js,.ts",
        "lint:fix": "pnpm lint --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "test": "jest --passWithNoTests",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@nodeboot/authorization": "^1.3.3",
        "@nodeboot/config": "^1.3.3",
        "@nodeboot/context": "^2.0.0",
        "@nodeboot/core": "^1.15.2",
        "@nodeboot/di": "^1.4.3",
        "@nodeboot/error": "^1.3.0",
        "@nodeboot/express-server": "^1.9.2",
        "@nodeboot/starter-actuator": "^1.7.3",
        "@nodeboot/starter-openapi": "^2.4.4",
        "@nodeboot/starter-persistence": "^1.12.1",
        "body-parser": "^1.20.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cookie": "^1.0.2",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "helmet": "^8.1.0",
        "hpp": "^0.2.3",
        "minimist": "^1.2.8",
        "multer": "^2.0.2",
        "reflect-metadata": "^0.2.1",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.20",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@nodeboot/jest": "workspace:*",
        "@types/jest": "^29.5.11",
        "@types/supertest": "^6.0.2",
        "sinon": "^21.0.0",
        "axios": "^1.12.2",
        "jest": "^29.5.0",
        "mongodb": "^5.9.2",
        "testcontainers": "^11.7.2",
        "pactum": "^3.7.6",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.1"
    }
}
