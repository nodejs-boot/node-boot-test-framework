{
    "name": "@nodeboot/node-test-demo",
    "version": "1.5.0",
    "description": "Demo project for Node-Boot tests with node:test",
    "author": "Manuel Santos <ney.br.santos@gmail.com>",
    "license": "MIT",
    "private": "true",
    "keywords": [
        "tests",
        "node-boot",
        "node:test"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nodejs-boot/node-boot-test-framework.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "types": "src/index.ts",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "clean:build": "rimraf ./dist",
        "dev": "nodemon",
        "lint": "eslint . --ext .js,.ts",
        "lint:fix": "pnpm lint --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "test": "node --test --test-concurrency=1 --require ts-node/register test/**/*.test.ts",
        "typecheck": "tsc",
        "nodeboot:update": "pnpm update @nodeboot/*@latest"
    },
    "dependencies": {
        "@nodeboot/authorization": "^1.3.4",
        "@nodeboot/config": "^1.3.4",
        "@nodeboot/context": "^2.1.0",
        "@nodeboot/core": "^1.15.3",
        "@nodeboot/di": "^1.4.4",
        "@nodeboot/error": "^1.3.0",
        "@nodeboot/express-server": "^1.9.3",
        "@nodeboot/starter-actuator": "^1.7.4",
        "@nodeboot/starter-openapi": "^2.4.5",
        "@nodeboot/starter-persistence": "^1.13.1",
        "better-sqlite3": "^9.6.0",
        "body-parser": "^1.20.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cookie": "^1.0.2",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "helmet": "^8.1.0",
        "hpp": "^0.2.3",
        "minimist": "^1.2.8",
        "mongodb": "^5.9.2",
        "multer": "^2.0.2",
        "reflect-metadata": "^0.2.1",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.20",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@nodeboot/node-test": "workspace:*",
        "@nodeboot/test": "workspace:*",
        "@types/supertest": "^6.0.2",
        "@types/node": "^24.9.2",
        "axios": "^1.12.2",
        "mongodb": "^5.9.2",
        "mongodb-memory-server": "^8.16.1",
        "testcontainers": "^11.7.2",
        "pactum": "^3.7.6",
        "supertest": "^7.0.0",
        "ts-node": "^10.9.2"
    }
}
